<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/users_nav_graph"
    app:startDestination="@id/usersList2">
    <fragment
        android:id="@+id/usersList2"
        android:name="com.example.myapplication.presentation.fragments.UsersList"
        android:label="fragment_users_list"
        tools:layout="@layout/fragment_users_list" >
        <action
            android:id="@+id/action_usersList2_to_userProfile2"
            app:destination="@id/userProfile2" />
    </fragment>
    <fragment
        android:id="@+id/savedUsers2"
        android:name="com.example.myapplication.presentation.fragments.SavedUsers"
        android:label="fragment_saved_users"
        tools:layout="@layout/fragment_saved_users" >
        <action
            android:id="@+id/action_savedUsers2_to_userProfile2"
            app:destination="@id/userProfile2" />
    </fragment>
    <fragment
        android:id="@+id/userProfile2"
        android:name="com.example.myapplication.presentation.fragments.UserProfile"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" />
</navigation>